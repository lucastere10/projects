programa
{
	funcao inicio()
	{
		inteiro n, x, y = 1, contador
		leia(n)
		enquanto (y < n)
		{
			para(contador = 1; contador <= n; contador += 1)
			{
				escreva("*")
			}
			escreva("\n")
			y++
		}
		escreva("III\nIII\nIII\n")
	}
}
